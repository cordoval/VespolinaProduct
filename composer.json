{
    "name": "vespolina/product",
    "description": "Vespolina library for working with Products.",
    "authors": [
        {"name": "Richard Shank", "email": "develop@zestic.com"}
    ],
    "keywords": ["Vespolina", "product", "ecommerce"],
    "require": {
        "iampersistent/gateway": "*",
        "php":                   ">=5.3.0",
        "vespolina/core":      "*"
    }, 
    "repositories":[
        {
            "type": "package",
            "package":{
                "version": "dev-master",
                "name": "vespolina/core",
                "source":{
                    "url": "git://github.com/iampersistent/VespolinaCore.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Vespolina": "lib/" }
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Vespolina": "lib/"
        }
    }
}